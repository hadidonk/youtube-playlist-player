{
  "name": "youtube-playlist-player",
  "productName": "YouTube Playlist Player",
  "version": "1.2.5",
  "description": "Watch and edit your YouTube playlist on the desktop",
  "license": "MIT",
  "homepage": "https://sidneys.github.io/youtube-playlist-player",
  "author": {
    "name": "sidneys",
    "email": "sidneys.github.io@outlook.com",
    "url": "https://github.com/sidneys"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sidneys/youtube-playlist-player.git"
  },
  "bugs": {
    "url": "https://github.com/sidneys/youtube-playlist-player/issues"
  },
  "engines": {
    "node": ">=7.0.0"
  },
  "os": [
    "darwin",
    "win32",
    "linux"
  ],
  "keywords": [
    "youtube",
    "always on top"
  ],
  "preferGlobal": true,
  "dependencies": {
    "app-root-path": "^2.0.1",
    "appdirectory": "^0.1.0",
    "babel-cli": "^6.24.1",
    "babel-preset-electron": "^1.4.15",
    "chalk": "^1.1.3",
    "chalkline": "0.0.5",
    "electron-editor-context-menu": "^1.1.1",
    "electron-settings": "^3.0.14",
    "electron-updater": "^1.14.0",
    "file-type": "^4.2.0",
    "file-url": "^2.0.2",
    "fs-extra": "^2.1.2",
    "get-youtube-id": "^1.0.0",
    "jquery": "^3.2.1",
    "keypath": "^0.0.1",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "parse-domain": "^1.1.0",
    "parse-semver": "^1.1.1",
    "present": "^1.0.0",
    "read-chunk": "^2.0.0",
    "semver-compare": "^1.0.0",
    "simple-reload": "0.0.4",
    "try-require": "latest"
  },
  "devDependencies": {
    "electron": "^1.6.7",
    "electron-builder": "^17.0.2",
    "electron-connect": "^0.6.1",
    "electron-prebuilt-compile": "git://github.com/sidneys/electron-prebuilt-compile.git#deploy",
    "fkill": "^4.1.0",
    "git-branch": "^0.3.0",
    "glob": "^7.1.1",
    "gulp": "^3.9.1",
    "gulp-load-plugins": "^1.5.0",
    "hash-files": "^1.1.1",
    "is-ci": "^1.0.10",
    "json2md": "^1.5.8",
    "progress": "^2.0.0",
    "publish-release": "^1.3.3",
    "yamljs": "^0.2.10"
  },
  "main": "./app/scripts/main/components/application.js",
  "bin": {
    "youtube-playlist-player": "./bin/cli.js"
  },
  "scripts": {
    "build": "node ./lib/build.js",
    "deploy": "node ./lib/deploy.js",
    "livereload": "./node_modules/.bin/gulp",
    "localsetup": "./node_modules/.bin/babel-node ./lib/localsetup.js",
    "releasenotes": "node ./lib/releasenotes.js",
    "dev": "./node_modules/.bin/electron ./app/scripts/main/components/application.js"
  },
  "build": {
    "appId": "de.sidneys.youtube-playlist-player",
    "directories": {
      "buildResources": "./build/staging",
      "output": "./build/output"
    },
    "files": [
      "!build",
      "!gh-pages",
      "!resources",
      "!website"
    ],
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "./icons/darwin/icon.icns",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "icon": "./icons/win32/icon.ico",
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "category": "Utility",
      "icon": "./icons/linux",
      "target": [
        "AppImage",
        "deb",
        "pacman",
        "rpm"
      ]
    },
    "dmg": {
      "background": "./icons/darwin/background-setup.png",
      "icon": "./icons/darwin/icon-setup.icns"
    },
    "nsis": {
      "artifactName": "${name}-${version}-setup.${ext}",
      "deleteAppDataOnUninstall": true,
      "installerHeader": "./icons/win32/header-setup.bmp",
      "installerHeaderIcon": "./icons/win32/icon.ico",
      "installerIcon": "./icons/win32/icon-setup.ico",
      "installerSidebar": "./icons/win32/background-setup.bmp",
      "menuCategory": "Pushbullet",
      "oneClick": false,
      "runAfterFinish": false
    }
  }
}
