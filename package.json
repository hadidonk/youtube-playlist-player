{
  "name": "yt-playlist-pro",
  "productName": "YT Playlist Pro",
  "version": "0.2.5",
  "description": "Watch & edit your YouTube playlist on the desktop",
  "license": "MIT",
  "author": {
    "name": "sidneys",
    "url": "http://sidneys.github.io"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sidneys/yt-playlist-pro.git"
  },
  "bugs": {
    "url": "https://github.com/sidneys/yt-playlist-pro/issues"
  },
  "homepage": "https://sidneys.github.io/yt-playlist-pro",
  "os": [
    "darwin",
    "win32",
    "linux"
  ],
  "keywords": [
    "youtube",
    "always on top"
  ],
  "engines": {
    "node": ">=7.0.0"
  },
  "preferGlobal": true,
  "dependencies": {
    "app-root-path": "^2.0.1",
    "appdirectory": "^0.1.0",
    "babel-cli": "^6.18.0",
    "babel-preset-electron": "^0.37.8",
    "chalk": "^1.1.3",
    "electron": "^1.4.13",
    "electron-compile": "^5.0.0",
    "electron-compilers": "5.0.0",
    "electron-connect": "^0.6.1",
    "electron-debug": "^1.1.0",
    "electron-editor-context-menu": "^1.1.1",
    "electron-packager": "^8.4.0",
    "electron-settings": "^2.2.2",
    "electron-squirrel-startup": "^1.0.0",
    "file-url": "^2.0.0",
    "fkill": "^4.1.0",
    "fs-extra": "^1.0.0",
    "glob": "^7.1.1",
    "gulp": "^3.9.1",
    "gulp-load-plugins": "^1.4.0",
    "keypath": "^0.0.1",
    "lodash": "^4.17.2",
    "node-archs": "^1.0.0",
    "node-platforms": "^2.0.0",
    "normalize-url": "^1.8.0",
    "open": "0.0.5",
    "progress": "^1.1.8",
    "publish-release": "^1.3.3",
    "rimraf": "^2.5.4",
    "try-require": "^1.2.1",
    "urlglob": "^0.1.0"
  },
  "optionalDependencies": {
    "appdmg": "^0.4.5",
    "electron-installer-debian": "^0.4.0",
    "electron-winstaller": "^2.5.1"
  },
  "main": "./app/es6-init.js",
  "bin": {
    "youtube-on-top": "./bin/cli.js"
  },
  "scripts": {
    "deploy:github": "node ./lib/deploy-github.js -- ",
    "start": "./node_modules/.bin/electron ./app/es6-init.js",
    "start:dev": "./node_modules/.bin/electron ./app/es6-init.js",
    "start:livereload": "./node_modules/.bin/gulp livereload",
    "build": "node ./lib/build.js -- ",
    "localsetup": "./node_modules/.bin/babel-node ./lib/localsetup.js -- ",
    "localsetup:dev": "./node_modules/.bin/babel-node ./lib/localsetup.js -- ",
    "localsetup:rebuild": "./node_modules/.bin/babel-node ./lib/localsetup.js -- ",
    "localsetup:rebuild:dev": "./node_modules/.bin/babel-node ./lib/localsetup.js -- "
  },
  "build": {
    "electronVersion": "1.4.13",
    "productName": "YT Playlist Pro",
    "id": "de.sidneys.yt-playlist-pro",
    "company": "sidneys.github.io",
    "category": "public.app-category.utilities",
    "directoryBuild": "build",
    "directoryIcons": "icons",
    "iconUrl": "https://raw.githubusercontent.com/sidneys/yt-playlist-pro/release/icons/win32/icon-app.ico"
  }
