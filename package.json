{
  "name": "youtube-playlist-player",
  "productName": "YouTube Playlist Player",
  "version": "0.7.0",
  "description": "Watch and edit your YouTube playlist on the desktop",
  "license": "MIT",
  "homepage": "https://sidneys.github.io/youtube-playlist-player",
  "author": {
    "name": "sidneys",
    "email": "sidneys.github.io@outlook.com",
    "url": "https://github.com/sidneys"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sidneys/youtube-playlist-player.git"
  },
  "bugs": {
    "url": "https://github.com/sidneys/youtube-playlist-player/issues"
  },
  "engines": {
    "node": ">=7.0.0"
  },
  "os": [
    "darwin",
    "win32",
    "linux"
  ],
  "keywords": [
    "youtube",
    "always on top"
  ],
  "preferGlobal": true,
  "dependencies": {
    "app-root-path": "^2.0.1",
    "appdirectory": "^0.1.0",
    "babel-cli": "^6.23.0",
    "babel-preset-electron": "^1.4.15",
    "chalk": "^1.1.3",
    "electron-auto-updater": "^1.0.0",
    "electron-compile": "^6.1.1",
    "electron-compilers": "^5.5.1",
    "electron-connect": "^0.6.1",
    "electron-editor-context-menu": "^1.1.1",
    "electron-settings": "^2.2.2",
    "electron-squirrel-startup": "^1.0.0",
    "fs-extra": "^2.0.0",
    "keypath": "0.0.1",
    "lodash": "^4.17.4",
    "normalize-url": "^1.9.0",
    "semver-compare": "^1.0.0",
    "semver-regex": "^1.0.0"
  },
  "devDependencies": {
    "electron": "^1.4.15",
    "electron-builder": "11.5.1",
    "electron-builder-squirrel-windows": "11.5.0",
    "fkill": "^4.1.0",
    "glob": "^7.1.1",
    "gulp": "^3.9.1",
    "gulp-load-plugins": "^1.5.0",
    "minimist": "^1.2.0",
    "progress": "^1.1.8",
    "publish-release": "^1.3.3"
  },
  "main": "./app/es6-init.js",
  "bin": {
    "youtube-playlist-player": "./bin/cli.js"
  },
  "scripts": {
    "deploy:github": "node ./lib/deploy-github.js -- ",
    "start": "./node_modules/.bin/electron ./app/es6-init.js",
    "start:dev": "./node_modules/.bin/electron ./app/es6-init.js",
    "start:livereload": "./node_modules/.bin/gulp livereload",
    "build": "node ./lib/build.js -- ",
    "localsetup": "./node_modules/.bin/babel-node ./lib/localsetup.js -- ",
    "localsetup:dev": "./node_modules/.bin/babel-node ./lib/localsetup.js -- ",
    "localsetup:rebuild": "./node_modules/.bin/babel-node ./lib/localsetup.js -- ",
    "localsetup:rebuild:dev": "./node_modules/.bin/babel-node ./lib/localsetup.js -- "
  },
  "build": {
    "appId": "de.sidneys.youtube-playlist-player",
    "directories": {
      "buildResources": "./build/staging",
      "output": "./build/output"
    },
    "files": [
      "!build",
      "!gh-pages",
      "!resources",
      "!website"
    ],
    "mac": {
      "icon": "./icons/darwin/icon.icns",
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.utilities"
    },
    "win": {
      "icon": "./icons/win32/icon.ico",
      "target": [
        "squirrel"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "pacman",
        "rpm"
      ],
      "category": "Utility"
    },
    "AppImage": {
      "includeRequiredLib": true
    },
    "deb": {
      "depends": [
        "libappindicator1",
        "libgconf-2-4",
        "libnotify-bin",
        "libnotify4"
      ]
    },
    "dmg": {
      "background": "./icons/darwin/background-setup.png",
      "icon": "./icons/darwin/icon-setup.icns"
    },
    "pacman": {
      "depends": [
        "libappindicator-gtk2",
        "libappindicator-gtk3",
        "libappindicator-sharp",
        "libnotify"
      ]
    },
    "rpm": {
      "depends": [
        "libappindicator1",
        "libgconf-2-4",
        "libnotify-bin",
        "libnotify4"
      ]
    },
    "squirrelWindows": {
      "iconUrl": "https://raw.githubusercontent.com/sidneys/youtube-playlist-player/release/icons/win32/icon.ico",
      "loadingGif": "./icons/win32/background-setup.gif"
    }
  }
}
